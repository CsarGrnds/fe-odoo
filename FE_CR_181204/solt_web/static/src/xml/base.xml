<?xml version="1.0" encoding="UTF-8"?>
<templates>

	<span t-name="FieldIconList" t-att-id="field_icon_list" class="oe_form_field oe_form_icon_list"
		t-att-style="widget.node.attrs.style">
		<t t-if="!widget.get('effective_readonly')">
			<select t-att-name="widget.name" class="oe_form_icon_list"
				t-att-tabindex="widget.node.attrs.tabindex" t-att-autofocus="widget.node.attrs.autofocus"
				t-att-id="widget.id_for_label">
				<t t-foreach="widget.values" t-as="option">
					<option>
						<t t-esc="widget.node.attrs.placeholder" t-if="option[0] === false and widget.node.attrs.placeholder" />
						<t t-esc="option[1]" t-if="option[0] !== false" />
					</option>
				</t>
			</select>
		</t>
		<img class="oe_form_icon_img" />
	</span>
</templates>
